# Gemini API
GOOGLE_API_KEY=your_gemini_api_key

# TikTok Marketing API (from https://developers.tiktok.com/apps)
TIKTOK_APP_ID=your_app_id
TIKTOK_APP_SECRET=your_app_secret
TIKTOK_ACCESS_TOKEN=obtain_via_oauth_server
TIKTOK_ADVERTISER_ID=obtain_via_oauth_server
TIKTOK_MOCK_MODE=true

# Logging
LOG_LEVEL=INFO